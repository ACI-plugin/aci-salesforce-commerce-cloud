                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="ACI_ForceResultCode">
                <display-name xml:lang="x-default">ACI ForceResultCode</display-name>
                <description xml:lang="x-default">Setting this value will force ACI to return specific error codes for example, 800.100.100. This is only for testing purposes and should not be set in LIVE environment</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="ACI_MerchantCity">
                <display-name xml:lang="x-default">ACI Merchant City</display-name>
