                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="ACI_UseSummary">
                <display-name xml:lang="x-default">Summary page in checkout flow?</display-name>
