                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="ACI_MerchantCountry">
                <display-name xml:lang="x-default">ACI Merchant Country</display-name>
                <description xml:lang="x-default">The country of the merchant.  This can be configured to override the merchant country shown in the card holder statement in ACI</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="ACI_MerchantMCC">
                <display-name xml:lang="x-default">ACI Merchant Category code</display-name>
                <description xml:lang="x-default">The merchants's category code. This can be configured to override the merchant category code shown in the card holder statement in ACI</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="ACI_MerchantName">
